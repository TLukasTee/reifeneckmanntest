{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/assets/Logo.jsx",
    "src/assets/Logo.js",
    "custom.d.ts"
, "src/app/page.tsx", "src/components/Hero.jsx", "src/components/Navbar.tsx", "src/components/Navbar.tsx", "src/app/terminkalender/page.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
